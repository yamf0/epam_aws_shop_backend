{
  "name": "infra",
  "version": "0.1.0",
  "bin": {
    "infra": "bin/infra.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "cdk:deploy:dynamo": "npm run build && cdk deploy DynamoStack",
    "cdk:deploy:lambda": "npm run build && cdk deploy LambdaStack",
    "cdk:deploy:import": "npm run build && cdk deploy ImportServiceStack",
    "cdk:deploy": "npm run build && cdk deploy --all",
    "cdk:destroy": "cdk destroy --all",
    "cdk:destroy:import": "cdk destroy ImportServiceStack"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.7.9",
    "aws-cdk": "2.1012.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.806.0",
    "@aws-sdk/client-s3": "^3.812.0",
    "@aws-sdk/lib-dynamodb": "^3.806.0",
    "@aws-sdk/s3-request-presigner": "^3.812.0",
    "@types/papaparse": "^5.3.16",
    "@types/uuid": "^10.0.0",
    "aws-cdk-lib": "2.190.0",
    "constructs": "^10.0.0",
    "papaparse": "^5.5.3",
    "uuid": "^11.1.0"
  }
